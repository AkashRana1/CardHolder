<h1>Create new User</h1>
<%= form_with(model: @user, local: true) do |f| %>
<div class="form-group">
  <%= f.label :email %><br/>
  <%= f.email_field :email, class: "form-control", :required => true, autofocus: true  %>
</div>
<div class="form-group">
  <%= f.label :password %><br/>
  <%= f.password_field :password, class: "form-control", :required => true, autofocus: true  %>
</div>
 
<div class="form-group">
  <%= f.submit %>
</div>
 
<% end %>
<%= button_to "Sign in", '/login', :method => "get" %>